{
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "cSpell.enabled": true,
  "cSpell.ignorePaths": [
    ".github",
    ".vscode"
  ],
  "editor.formatOnSave": true,
  "omnisharp.useEditorFormattingSettings": false,
  "dotnet.defaultSolution": "WebApi.sln",
  "files.exclude": {
    "**/bin": true,
    "**/obj": true,
    "**/Properties": true,
    "**/.vs": true,
  },
}