{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "start dotnet",
      "command": "sh .vscode/start-proc.sh",
      "isBackground": true,
      "problemMatcher": {
        "pattern": [
          {
            "regexp": ".",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*",
          "endsPattern": "^.*Content root path"
        }
      },
    }
  ]
}